@prefix : <http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sdj: <http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#> .

<http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres> rdf:type owl:Ontology ;
                                                                                     dc:creator "Giacometti Lorenzo" ,
                                                                                                "Giovanale Matteo" ;
                                                                                     dc:date "2025-08-03T09:00:00"^^xsd:dateTime ;
                                                                                     dc:description "Un'ontologia per rappresentare i giochi da tavolo vincitori del premio tedesco Spiel des Jahres"@it ;
                                                                                     dc:title "Ontologia dei Giochi da Tavolo Spiel des Jahres"@it ;
                                                                                     vann:preferredNamespacePrefix "http://www.boardgames.it/ontologies/spiel-des-jahres#" ,
                                                                                                                   "sdj" ;
                                                                                     rdfs:label "Spiel des Jahres Ontology"@it .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#title
:title rdf:type owl:AnnotationProperty ;
       rdfs:subPropertyOf dc:title ;
       rdfs:range xsd:string .


###  http://www.w3.org/2000/01/rdf-schema#label
rdfs:label rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/name
foaf:name rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#atLocation
:atLocation rdf:type owl:ObjectProperty ;
            rdfs:domain :TemporalSituation ;
            rdfs:range :Location ;
            rdfs:comment "Luogo dove si verifica l'evento"@it ;
            rdfs:label "al luogo"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#belongsToGenre
:belongsToGenre rdf:type owl:ObjectProperty ;
                rdfs:domain :BoardGame ;
                rdfs:range :GameGenre ;
                rdfs:comment "Un gioco appartiene a un genere specifico"@it ;
                rdfs:label "appartiene al genere"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#coDesignedBy
:coDesignedBy rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :BoardGame ;
              rdfs:range :GameDesigner ;
              rdfs:comment "Un gioco può essere co-progettato da al massimo un co-designer"@it ;
              rdfs:label "co-progettato da"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#designedBy
:designedBy rdf:type owl:ObjectProperty ;
            owl:inverseOf :hasDesigned ;
            rdfs:domain :BoardGame ;
            rdfs:range :GameDesigner ;
            rdfs:comment "Relazione tra un gioco e il suo designer"@it ;
            rdfs:label "progettato da"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#designedByWinner
:designedByWinner rdf:type owl:ObjectProperty ;
                  rdfs:domain :BoardGame ;
                  rdfs:range :GameDesigner ;
                  owl:propertyChainAxiom ( :designedBy
                                           :hasDesigned
                                           :wonAward
                                           [ owl:inverseOf :wonAward
                                           ]
                                           [ owl:inverseOf :hasDesigned
                                           ]
                                         ) ;
                  rdfs:comment "Un gioco è progettato da un designer che ha vinto almeno un premio"@it ;
                  rdfs:label "progettato da vincitore"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#designerOfAwardWinningGame
:designerOfAwardWinningGame rdf:type owl:ObjectProperty ;
                            rdfs:domain :GameDesigner ;
                            rdfs:range :GameAward ;
                            owl:propertyChainAxiom ( :hasDesigned
                                                     :wonAward
                                                   ) ;
                            rdfs:comment "Se un designer ha progettato un gioco che ha vinto un premio, allora il designer è collegato a quel premio"@it ;
                            rdfs:label "designer di vincitore premio"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#hasAwardCeremony
:hasAwardCeremony rdf:type owl:ObjectProperty ;
                  rdfs:domain :GameAward ;
                  rdfs:range :AwardCeremony ;
                  rdfs:comment "Un premio ha una cerimonia di assegnazione"@it ;
                  rdfs:label "ha cerimonia premio"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#hasCollaboratedWith
:hasCollaboratedWith rdf:type owl:ObjectProperty ;
                     rdfs:domain :GameDesigner ;
                     rdfs:range :GameDesigner ;
                     owl:propertyChainAxiom ( :hasDesigned
                                              :coDesignedBy
                                            ) ,
                                            ( [ owl:inverseOf :coDesignedBy
                                              ]
                                              :designedBy
                                            ) ;
                     rdfs:comment "Se un designer ha progettato un gioco che è stato co-progettato da un altro designer, allora il primo ha collaborato con il secondo"@it ;
                     rdfs:label "ha collaborato con"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#hasDesigned
:hasDesigned rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf foaf:made ;
             rdfs:domain :GameDesigner ;
             rdfs:range :BoardGame ;
             rdfs:comment "Relazione inversa: un designer ha progettato un gioco"@it ;
             rdfs:label "ha progettato"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#hasExpansion
:hasExpansion rdf:type owl:ObjectProperty ;
              rdfs:domain :BoardGame ;
              rdfs:range :GameExpansion ;
              rdfs:comment "Un gioco ha delle espansioni"@it ;
              rdfs:label "ha espansione"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#hasItem
:hasItem rdf:type owl:ObjectProperty ;
         rdfs:domain :NominationList ;
         rdfs:range :ListItem ;
         rdfs:comment "Collega una lista ai suoi elementi"@it ;
         rdfs:label "ha elemento"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#hasReleaseEvent
:hasReleaseEvent rdf:type owl:ObjectProperty ;
                 rdfs:domain :BoardGame ;
                 rdfs:range :GameRelease ;
                 rdfs:comment "Un gioco ha un evento di rilascio"@it ;
                 rdfs:label "ha evento rilascio"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#itemContent
:itemContent rdf:type owl:ObjectProperty ;
             rdfs:domain :ListItem ;
             rdfs:range :BoardGame ;
             rdfs:comment "Il gioco contenuto nell'elemento della lista"@it ;
             rdfs:label "contenuto elemento"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#nominatedFor
:nominatedFor rdf:type owl:ObjectProperty ;
              rdfs:domain :BoardGame ;
              rdfs:range :GameAward ;
              rdfs:comment "un gioco è stato nominato per il premio ma non lo ha vinto"@it ;
              rdfs:label "nominato a"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#publishedBy
:publishedBy rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :BoardGame ;
             rdfs:range :Publisher ;
             rdfs:comment "Un gioco è pubblicato da un singolo editore"@it ;
             rdfs:label "pubblicato da"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#publisherOfWinningGame
:publisherOfWinningGame rdf:type owl:ObjectProperty ;
                        rdfs:domain :Publisher ;
                        rdfs:range :GameAward ;
                        owl:propertyChainAxiom ( [ owl:inverseOf :publishedBy
                                                 ]
                                                 :wonAward
                                               ) ;
                        rdfs:comment "Se un editore pubblica un gioco che ha vinto un premio, allora l'editore è collegato a quel premio"@it ;
                        rdfs:label "editore di gioco vincitore"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#relatedAward
:relatedAward rdf:type owl:ObjectProperty ;
              rdfs:domain :NominationList ;
              rdfs:range :GameAward ;
              rdfs:comment "Il premio a cui si riferisce la lista di nomination"@it ;
              rdfs:label "premio correlato"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#situationParticipant
:situationParticipant rdf:type owl:ObjectProperty ;
                      rdfs:domain :TemporalSituation ;
                      rdfs:comment "Entità che partecipa alla situazione temporale"@it ;
                      rdfs:label "partecipante situazione"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#wonAward
:wonAward rdf:type owl:ObjectProperty ;
          rdfs:domain :BoardGame ;
          rdfs:range :GameAward ;
          rdfs:comment "Un gioco ha vinto un determinato premio"@it ;
          rdfs:label "ha vinto premio"@it .


###  http://xmlns.com/foaf/0.1/made
foaf:made rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#atTime
:atTime rdf:type owl:DatatypeProperty ;
        rdfs:domain :TemporalSituation ;
        rdfs:range xsd:dateTime ;
        rdfs:comment "Momento esatto dell'evento"@it ;
        rdfs:label "al momento"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#awardYear
:awardYear rdf:type owl:DatatypeProperty ;
           rdfs:domain :SpielDesJahresWinner ;
           rdfs:range xsd:gYear ;
           rdfs:comment "L'anno in cui ha vinto il Spiel des Jahres"@it ;
           rdfs:label "anno del premio"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#bggRating
:bggRating rdf:type owl:DatatypeProperty ;
           rdfs:domain :BoardGame ;
           rdfs:range xsd:decimal ;
           rdfs:comment "Valutazione media su BoardGameGeek"@it ;
           rdfs:label "valutazione BGG"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#complexity
:complexity rdf:type owl:DatatypeProperty ;
            rdfs:domain :BoardGame ;
            rdfs:range xsd:decimal ;
            rdfs:comment "Livello di complessità del gioco (scala da 1.0 a 5.0)" ;
            rdfs:label "complessità"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#eventYear
:eventYear rdf:type owl:DatatypeProperty ;
           rdfs:domain :AwardEvent ;
           rdfs:range xsd:gYear ;
           rdfs:comment "anno in cui si è svolta la premiazione" ;
           rdfs:label "anno dell'evento"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#itemPosition
:itemPosition rdf:type owl:DatatypeProperty ;
              rdfs:domain :ListItem ;
              rdfs:range xsd:positiveInteger ;
              rdfs:comment "Posizione ordinale nell'elenco (1=primo, ovvero vincitore)"@it ;
              rdfs:label "posizione elemento"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#maxPlayers
:maxPlayers rdf:type owl:DatatypeProperty ;
            rdfs:domain :BoardGame ;
            rdfs:range xsd:nonNegativeInteger ;
            rdfs:comment "Il numero massimo di giocatori per questo gioco"@it ;
            rdfs:label "numero massimo di giocatori"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#minPlayers
:minPlayers rdf:type owl:DatatypeProperty ;
            rdfs:domain :BoardGame ;
            rdfs:range xsd:nonNegativeInteger ;
            rdfs:comment "Il numero minimo di giocatori per questo gioco"@it ;
            rdfs:label "numero minimo giocatori"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#playingTime
:playingTime rdf:type owl:DatatypeProperty ;
             rdfs:domain :BoardGame ;
             rdfs:range xsd:positiveInteger ;
             rdfs:comment "Durata media del gioco in minuti"@it ;
             rdfs:label "tempo di gioco" .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#publicationYear
:publicationYear rdf:type owl:DatatypeProperty ;
                 rdfs:domain :BoardGame ;
                 rdfs:range xsd:gYear ;
                 rdfs:comment "L'anno in cui è stato pubblicato il gioco"@it ;
                 rdfs:label "anno di pubblicazione"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#title
:title rdf:type owl:DatatypeProperty ;
       rdfs:domain :BoardGame .


#################################################################
#    Classes
#################################################################

###  http://purl.org/dc/terms/Agent
dcterms:Agent rdf:type owl:Class .


###  http://schema.org/CreativeWork
<http://schema.org/CreativeWork> rdf:type owl:Class .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#AwardCeremony
:AwardCeremony rdf:type owl:Class ;
               rdfs:subClassOf :TemporalSituation ;
               rdfs:comment "Cerimonia di assegnazione di un premio"@it ;
               rdfs:label "Cerimonia di Premiazione"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#AwardEvent
:AwardEvent rdf:type owl:Class ;
            rdfs:comment "L'evento di assegnazione di un premio"@it ;
            rdfs:label "Evento Premio"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#BoardGame
:BoardGame rdf:type owl:Class ;
           rdfs:subClassOf <http://schema.org/CreativeWork> ;
           rdfs:comment "Un gioco che si svolge su di un tavolo con componenti fisici"@it ;
           rdfs:label "Gioco da Tavolo"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#ClassicSpielDesJahres
:ClassicSpielDesJahres rdf:type owl:Class ;
                       rdfs:subClassOf :SpielDesJahresWinner ;
                       rdfs:comment "Vincitore della categoria principale Spiel des Jahres"@it ;
                       rdfs:label "Spiel des Jahres Classico"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#CompetitiveGame
:CompetitiveGame rdf:type owl:Class ;
                 rdfs:subClassOf :BoardGame ;
                 rdfs:comment "Un gioco dove i giocatori competono l'uno contro l'altro"@it ;
                 rdfs:label "Gioco Competitivo"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#CooperativeGame
:CooperativeGame rdf:type owl:Class ;
                 rdfs:subClassOf :BoardGame ;
                 rdfs:comment "Un gioco dove i giocatori collaborano per raggiungere  un obiettivo comune" ;
                 rdfs:label "Gioco Cooperativo"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#EurogamePublisher
:EurogamePublisher rdf:type owl:Class ;
                   rdfs:subClassOf :Publisher ;
                   rdfs:comment "Casa editrice specializzata in eurogames" ;
                   rdfs:label "Editore di Eurogame" .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#GameAward
:GameAward rdf:type owl:Class ;
           rdfs:comment "Un premio specifico per giochi da tavolo"@it ;
           rdfs:label "Premio per giochi"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#GameDesigner
:GameDesigner rdf:type owl:Class ;
              rdfs:subClassOf dcterms:Agent ,
                              :Person ,
                              foaf:Person ;
              rdfs:comment "Una persona che progetta giochi da tavolo"@it ;
              rdfs:label "Designer di Giochi"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#GameExpansion
:GameExpansion rdf:type owl:Class ;
               rdfs:comment "Un'espansione che aggiunge contenuti a un gioco base"@it ;
               rdfs:label "Espansione di Gioco"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#GameGenre
:GameGenre rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:oneOf ( :Abstract
                                             :Adventure
                                             :Economic
                                             :Party
                                             :Strategy
                                           )
                               ] ;
           rdfs:subClassOf skos:Concept ;
           rdfs:comment "I principali generi di giochi da tavolo"@it ;
           rdfs:label "Genere di Gioco"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#GameRelease
:GameRelease rdf:type owl:Class ;
             rdfs:subClassOf :TemporalSituation ;
             rdfs:comment "Evento di pubblicazione di un gioco"@it ;
             rdfs:label "Rilascio Gioco"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#KennerspielDesJahres
:KennerspielDesJahres rdf:type owl:Class ;
                      rdfs:subClassOf :SpielDesJahresWinner ;
                      rdfs:comment "Vincitore della categoria Kennerspiel (giochi per esperti)"@it ;
                      rdfs:label "Kennerspiel des Jahres"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#KennerspielDesJahresNominee
:KennerspielDesJahresNominee rdf:type owl:Class ;
                             rdfs:subClassOf :SpielDesJahresNominee ;
                             rdfs:comment "Un gioco nominato per il premio Kennerspiel des Jahres"@it ;
                             rdfs:label "Nominato Kennerspiel des Jahres"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#ListItem
:ListItem rdf:type owl:Class ;
          rdfs:comment "Un elemento di una lista con posizione ordinale"@it ;
          rdfs:label "Elemento Lista"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#Location
:Location rdf:type owl:Class ;
          rdfs:comment "Un luogo geografico"@it ;
          rdfs:label "Luogo"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#NominationList
:NominationList rdf:type owl:Class ;
                rdfs:comment "Una lista ordinata di giochi nominati per un premio"@it ;
                rdfs:label "Lista Nomination"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#Person
:Person rdf:type owl:Class ;
        rdfs:comment "Una persona fisica coinvolta nel mondo dei giochi da tavolo"@it ;
        rdfs:label "Persona"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#PopularGame
:PopularGame rdf:type owl:Class ;
             rdfs:subClassOf :BoardGame ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :bggRating ;
                               owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                    owl:onDatatype xsd:decimal ;
                                                    owl:withRestrictions ( [ xsd:minInclusive 7.5
                                                                           ]
                                                                         )
                                                  ]
                             ] ;
             rdfs:comment "Un gioco con valutazione BGG superiore a 7.5"@it ;
             rdfs:label "Gioco Popolare" .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#ProvenDesigner
:ProvenDesigner rdf:type owl:Class ;
                rdfs:subClassOf :GameDesigner ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasDesigned ;
                                  owl:minQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                                  owl:onClass :BoardGame
                                ] ;
                rdfs:comment "Un designer che ha creato almeno 3 giochi"@it ;
                rdfs:label "Designer Esperto"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#Publisher
:Publisher rdf:type owl:Class ;
           rdfs:subClassOf dcterms:Agent ;
           rdfs:comment "Una casa editrice di giochi da tavolo"@it ;
           rdfs:label "Editore"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#RecentWinner
:RecentWinner rdf:type owl:Class ;
              rdfs:subClassOf :SpielDesJahresWinner ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :awardYear ;
                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                     owl:onDatatype xsd:gYear ;
                                                     owl:withRestrictions ( [ xsd:minInclusive "2024"^^xsd:gYear
                                                                            ]
                                                                          )
                                                   ]
                              ] ;
              rdfs:comment "Vincitore Spiel des Jahres dal 2024 in poi"@it ;
              rdfs:label "Vincitore Recente"@it .


###  http://www.semanticweb.org/giacomettigiovanale/ontologies/2025/7/spiel-des-jahres#SpielDesJahresNominee
:SpielDesJahresNominee rdf:type owl:Class ;
                       rdfs:subClassOf :BoardGame ;
                       rdfs:comment "Un gioco nominato per il premio Spiel des Jahres"@it ;
