                      owl:equivalentClass [ rdf:type owl:Restriction ;
                                            owl:onProperty :wonAward ;
                                            owl:someValuesFrom :GameAward
                                          ] ;
                      rdfs:subClassOf :BoardGame ;
                      rdfs:comment "Un gioco da tavolo che ha vinto il premio Spiel des Jahres"@it ;
                      rdfs:label "Vincitore Spiel des Jahres"@it .


