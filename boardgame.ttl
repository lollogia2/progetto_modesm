#################################################################
#    Rules
#################################################################

:g rdf:type swrl:Variable .

:comp rdf:type swrl:Variable .

:time rdf:type swrl:Variable .

:g1 rdf:type swrl:Variable .

:g2 rdf:type swrl:Variable .

:g3 rdf:type swrl:Variable .

:d rdf:type swrl:Variable .

:designer rdf:type swrl:Variable .

:game1 rdf:type swrl:Variable .

:game2 rdf:type swrl:Variable .

:award1 rdf:type swrl:Variable .

:award2 rdf:type swrl:Variable .

:p rdf:type swrl:Variable .

:genre1 rdf:type swrl:Variable .

:genre2 rdf:type swrl:Variable .

:d1 rdf:type swrl:Variable .

:d2 rdf:type swrl:Variable .

[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean ;
   rdfs:comment "Un gioco con complessità >= 3.0 e tempo di gioco > 90 minuti viene classificato come ExpertGame" ;
   rdfs:label "Regola 2 - Giochi per Esperti" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate :BoardGame ;
                           swrl:argument1 :g
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                                      swrl:propertyPredicate :complexity ;
                                      swrl:argument1 :g ;
                                      swrl:argument2 :comp
                                    ] ;
                          rdf:rest [ rdf:type swrl:AtomList ;
                                     rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                                                 swrl:propertyPredicate :playingTime ;
                                                 swrl:argument1 :g ;
                                                 swrl:argument2 :time
                                               ] ;
                                     rdf:rest [ rdf:type swrl:AtomList ;
                                                rdf:first [ rdf:type swrl:BuiltinAtom ;
                                                            swrl:builtin swrlb:greaterThanOrEqual ;
                                                            swrl:arguments [ rdf:type rdf:List ;
                                                                             rdf:first :comp ;
                                                                             rdf:rest [ rdf:type rdf:List ;
                                                                                        rdf:first 3.0 ;
                                                                                        rdf:rest rdf:nil
                                                                                      ]
                                                                           ]
                                                          ] ;
                                                rdf:rest [ rdf:type swrl:AtomList ;
                                                           rdf:first [ rdf:type swrl:BuiltinAtom ;
                                                                       swrl:builtin swrlb:greaterThan ;
                                                                       swrl:arguments [ rdf:type rdf:List ;
                                                                                        rdf:first :time ;
                                                                                        rdf:rest [ rdf:type rdf:List ;
                                                                                                   rdf:first 90 ;
                                                                                                   rdf:rest rdf:nil
                                                                                                 ]
                                                                                      ]
                                                                     ] ;
                                                           rdf:rest rdf:nil
                                                         ]
                                              ]
                                   ]
                        ]
             ] ;
   swrl:head [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate :ExpertGame ;
                           swrl:argument1 :g
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .

[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean ;
   rdfs:comment "regola per trovare  designer che hanno progettato almeno 3 giochi diversi" ;
   rdfs:label "Regola 6 - Designer Esperto" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate :BoardGame ;
                           swrl:argument1 :g1
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:ClassAtom ;
                                      swrl:classPredicate :BoardGame ;
                                      swrl:argument1 :g2
                                    ] ;
                          rdf:rest [ rdf:type swrl:AtomList ;
                                     rdf:first [ rdf:type swrl:ClassAtom ;
                                                 swrl:classPredicate :BoardGame ;
                                                 swrl:argument1 :g3
                                               ] ;
                                     rdf:rest [ rdf:type swrl:AtomList ;
                                                rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                            swrl:propertyPredicate :designedBy ;
                                                            swrl:argument1 :g1 ;
                                                            swrl:argument2 :d
                                                          ] ;
                                                rdf:rest [ rdf:type swrl:AtomList ;
                                                           rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                                       swrl:propertyPredicate :designedBy ;
                                                                       swrl:argument1 :g2 ;
                                                                       swrl:argument2 :d
                                                                     ] ;
                                                           rdf:rest [ rdf:type swrl:AtomList ;
                                                                      rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                                                  swrl:propertyPredicate :designedBy ;
                                                                                  swrl:argument1 :g3 ;
                                                                                  swrl:argument2 :d
                                                                                ] ;
                                                                      rdf:rest [ rdf:type swrl:AtomList ;
                                                                                 rdf:first [ rdf:type swrl:DifferentIndividualsAtom ;
                                                                                             swrl:argument1 :g1 ;
                                                                                             swrl:argument2 :g2
                                                                                           ] ;
                                                                                 rdf:rest [ rdf:type swrl:AtomList ;
                                                                                            rdf:first [ rdf:type swrl:DifferentIndividualsAtom ;
                                                                                                        swrl:argument1 :g1 ;
                                                                                                        swrl:argument2 :g3
                                                                                                      ] ;
                                                                                            rdf:rest [ rdf:type swrl:AtomList ;
                                                                                                       rdf:first [ rdf:type swrl:DifferentIndividualsAtom ;
                                                                                                                   swrl:argument1 :g2 ;
                                                                                                                   swrl:argument2 :g3
                                                                                                                 ] ;
                                                                                                       rdf:rest rdf:nil
                                                                                                     ]
                                                                                          ]
                                                                               ]
                                                                    ]
                                                         ]
                                              ]
                                   ]
                        ]
             ] ;
   swrl:head [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate :ExpertDesigner ;
                           swrl:argument1 :d
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .

[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean ;
   rdfs:comment "Un designer che ha progettato almeno 2 giochi che hanno vinto premi viene classificato come ProlificWinnerDesigner" ;
