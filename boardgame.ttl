[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :CompetitiveGame
                :CooperativeGame
                :GameExpansion
              )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :Abstract
                        :Adventure
                        :Economic
                        :Party
                        :Strategy
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :Agricola
